{
  "manifest_version": 3,
  "name": "Custom Search Shortcuts",
  "short_name": "Custom Search",
  "version": "1.1.0.0",
  "description": "Custom search shortcuts allows you to quickly search selected text on your favorite websites.",
  "author": "Vinay Chowdary Duvvada",
  "homepage_url": "https://github.com/dvinay/custom_search_shortcuts",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}